            </Expression>
            <Expression xsi:type="ipy:PythonTransform">
              <ipy:Description>states the minimum distance of the tadpole to the center of the dish for a stimulus to start</ipy:Description>
              <ipy:Script>import math

@returns(int)
def process(value):
  tadpole_x = value.X/1024
  tadpole_y = value.Y/1024
  distance_to_center = math.sqrt((tadpole_y-0.5)**2 + (tadpole_x-0.5)**2)

  if distance_to_center &gt; 0.3:
     return 0
  else :
     return 1</ipy:Script>
            </Expression>
            <Expression xsi:type="rx:PublishSubject">
              <Name>stim_area</Name>
