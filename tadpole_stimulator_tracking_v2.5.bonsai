            <Expression xsi:type="ipy:PythonTransform">
              <ipy:Description>returns the angle between the center of the dish and the animal</ipy:Description>
              <ipy:Script>import math
from System import Tuple

@returns(float)
def process(value):
  x, y = value
  angle_rad = math.atan2(0.5-y, 0.5-x)
  angle_rad = angle_rad+3.14159265359
  return float(angle_rad)</ipy:Script>
